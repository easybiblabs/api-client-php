{
    "name": "easybib/api-client-php",
    "description": "Client for EasyBib API, implemented in PHP",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Yitzchak Schaffer",
            "email": "yitzchak@imagineeasy.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:easybiblabs/oauth2-client-php.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/cache": "~1.3",
        "guzzlehttp/guzzle": "^6.0",
        "easybiblabs/oauth2-client-php": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7",
        "phpmd/phpmd": "^2.4.3",
        "squizlabs/php_codesniffer": "^2.6",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "EasyBib\\Api\\Client\\": "src/",
            "EasyBib\\Guzzle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "EasyBib\\Tests\\Api\\Client\\": "tests/",
            "EasyBib\\Tests\\Mocks\\": "tests/"
        },
        "classmap": ["tests/"]
    }
}
