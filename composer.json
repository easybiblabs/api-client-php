{
    "name": "easybib/api-client-php",
    "description": "Client for EasyBib API, implemented in PHP",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Yitzchak Schaffer",
            "email": "yitzchak@imagineeasy.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:fabpot/PHP-CS-Fixer.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:easybiblabs/oauth2-client-php.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "easybiblabs/oauth2-client-php": ">=0.3.1",
        "doctrine/cache": "~1.3",
        "guzzle/guzzle": "3.8.*"
    },
    "require-dev": {
      "phpunit/phpunit": ">=3.7",
      "phpmd/phpmd": "*",
      "fabpot/php-cs-fixer": "dev-master",
      "squizlabs/php_codesniffer": "1.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "EasyBib\\Api\\Client\\": "src/",
            "EasyBib\\Guzzle\\": "src/",
            "EasyBib\\Tests\\Api\\Client\\": "tests/"
        },
        "classmap": ["tests/"]
    }
}
